<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="417.0" prefWidth="626.0" xmlns="http://javafx.com/javafx/8.0.202-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="View_Controller.LoginController">
   <children>
      <Label fx:id="LabelTitle" layoutX="64.0" layoutY="51.0" prefHeight="22.0" prefWidth="347.0" text="Login Screen" />
      <AnchorPane layoutX="96.0" layoutY="111.0" prefHeight="200.0" prefWidth="399.0">
         <children>
            <TextField fx:id="FieldUsername" layoutX="192.0" layoutY="49.0" />
            <TextField fx:id="FieldPassword" layoutX="192.0" layoutY="109.0" />
            <Label fx:id="LabelUsername" layoutX="34.0" layoutY="54.0" prefHeight="22.0" prefWidth="149.0" text="Username" />
            <Label fx:id="LabelPassword" layoutX="36.0" layoutY="114.0" prefHeight="22.0" prefWidth="149.0" text="Password" />
            <Button fx:id="BtnLogin" layoutX="231.0" layoutY="152.0" mnemonicParsing="false" onMouseClicked="#setBtnLogin" text="Login" />
         </children>
      </AnchorPane>
      <Button fx:id="BtnExit" layoutX="464.0" layoutY="336.0" mnemonicParsing="false" text="Exit" visible="false" />
      <Button fx:id="BtnCheater" layoutX="479.0" layoutY="46.0" mnemonicParsing="false" onMouseClicked="#cheaterBtnPushed" text="Cheater Button" visible="false" />
   </children>
</AnchorPane>
